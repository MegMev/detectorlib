#!/bin/bash


GEO='REST_GEOMETRY_TYPE=traditional;REST_GEOMETRY_SETUP=pandaXIII_Setup_WT12m.gdml'

mkdir gas -p
 ./fillRmlVars isotopeFromVolume.rml gas/gas_Xe137.rml \
 "[REST_VOLUME=gas;REST_ISOTOPE=Xe137;REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=10]"
 ./fillRmlVars isotopeFromVolume.rml gas/gas_Co60.rml \
 "[REST_VOLUME=gas;REST_ISOTOPE=Co60; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=10]"
 ./fillRmlVars decay0FromVolume.rml gas/gas_Xe136bb0n.rml \
 "[REST_VOLUME=gas;REST_GENERATOR=Xe136bb0n; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=10]"
 ./fillRmlVars decay0FromVolume.rml gas/gas_Xe136bb2n.rml \
 "[REST_VOLUME=gas;REST_GENERATOR=Xe136bb2n; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=10]"

mkdir vessel -p
 ./fillRmlVars isotopeFromVolume.rml vessel/vessel_Co60.rml \
 "[REST_VOLUME=vessel;REST_ISOTOPE=Co60;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml vessel/vessel_Th232.rml \
 "[REST_VOLUME=vessel;REST_ISOTOPE=Th232; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml vessel/vessel_Ra228.rml \
 "[REST_VOLUME=vessel;REST_ISOTOPE=Ra228; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml vessel/vessel_U238.rml \
 "[REST_VOLUME=vessel;REST_ISOTOPE=U238;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml vessel/vessel_Th234.rml \
 "[REST_VOLUME=vessel;REST_ISOTOPE=Th234; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml vessel/vessel_Bi214.rml \
 "[REST_VOLUME=vessel;REST_ISOTOPE=Bi214; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml vessel/vessel_Tl208.rml \
 "[REST_VOLUME=vessel;REST_ISOTOPE=Tl208; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"

mkdir micromegas -p
 ./fillRmlVars isotopeFromVolume.rml micromegas/micromegas1_Co60.rml \
 "[REST_VOLUME=micromegas1;REST_ISOTOPE=Co60;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml micromegas/micromegas1_Th232.rml \
 "[REST_VOLUME=micromegas1;REST_ISOTOPE=Th232; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml micromegas/micromegas1_Ra228.rml \
 "[REST_VOLUME=micromegas1;REST_ISOTOPE=Ra228; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml micromegas/micromegas1_U238.rml \
 "[REST_VOLUME=micromegas1;REST_ISOTOPE=U238;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml micromegas/micromegas1_Th234.rml \
 "[REST_VOLUME=micromegas1;REST_ISOTOPE=Th234; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml micromegas/micromegas1_Bi214.rml \
 "[REST_VOLUME=micromegas1;REST_ISOTOPE=Bi214; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml micromegas/micromegas1_Tl208.rml \
 "[REST_VOLUME=micromegas1;REST_ISOTOPE=Tl208; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"

mkdir ring -p
 ./fillRmlVars isotopeFromVolume.rml ring/ring_Co60.rml \
 "[REST_VOLUME=ring;REST_ISOTOPE=Co60;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml ring/ring_Th232.rml \
 "[REST_VOLUME=ring;REST_ISOTOPE=Th232; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml ring/ring_Ra228.rml \
 "[REST_VOLUME=ring;REST_ISOTOPE=Ra228; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml ring/ring_U238.rml \
 "[REST_VOLUME=ring;REST_ISOTOPE=U238;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml ring/ring_Th234.rml \
 "[REST_VOLUME=ring;REST_ISOTOPE=Th234; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml ring/ring_Bi214.rml \
 "[REST_VOLUME=ring;REST_ISOTOPE=Bi214; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml ring/ring_Tl208.rml \
 "[REST_VOLUME=ring;REST_ISOTOPE=Tl208; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"

mkdir supports -p
 ./fillRmlVars isotopeFromVolume.rml supports/supports_Co60.rml \
 "[REST_VOLUME=supports;REST_ISOTOPE=Co60;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml supports/supports_Th232.rml \
 "[REST_VOLUME=supports;REST_ISOTOPE=Th232; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml supports/supports_Ra228.rml \
 "[REST_VOLUME=supports;REST_ISOTOPE=Ra228; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml supports/supports_U238.rml \
 "[REST_VOLUME=supports;REST_ISOTOPE=U238;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml supports/supports_Th234.rml \
 "[REST_VOLUME=supports;REST_ISOTOPE=Th234; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml supports/supports_Bi214.rml \
 "[REST_VOLUME=supports;REST_ISOTOPE=Bi214; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"
 ./fillRmlVars isotopeFromVolume.rml supports/supports_Tl208.rml \
 "[REST_VOLUME=supports;REST_ISOTOPE=Tl208; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50]"

mkdir electronics -p
 ./fillRmlVars isotopeFromPoint.rml electronics/electronics_Co60.rml \
 "[REST_VOLUME=electronics;REST_ISOTOPE=Co60;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50;REST_X=0;REST_Y=0;REST_Z=-1150]"
 ./fillRmlVars isotopeFromPoint.rml electronics/electronics_Th232.rml \
 "[REST_VOLUME=electronics;REST_ISOTOPE=Th232; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50;REST_X=0;REST_Y=0;REST_Z=-1150]"
 ./fillRmlVars isotopeFromPoint.rml electronics/electronics_Ra228.rml \
 "[REST_VOLUME=electronics;REST_ISOTOPE=Ra228; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50;REST_X=0;REST_Y=0;REST_Z=-1150]"
 ./fillRmlVars isotopeFromPoint.rml electronics/electronics_U238.rml \
 "[REST_VOLUME=electronics;REST_ISOTOPE=U238;  REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50;REST_X=0;REST_Y=0;REST_Z=-1150]"
 ./fillRmlVars isotopeFromPoint.rml electronics/electronics_Th234.rml \
 "[REST_VOLUME=electronics;REST_ISOTOPE=Th234; REST_NEVENTS=100000;REST_FULLCHAIN=on; $GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50;REST_X=0;REST_Y=0;REST_Z=-1150]"
 ./fillRmlVars isotopeFromPoint.rml electronics/electronics_Bi214.rml \
 "[REST_VOLUME=electronics;REST_ISOTOPE=Bi214; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50;REST_X=0;REST_Y=0;REST_Z=-1150]"
 ./fillRmlVars isotopeFromPoint.rml electronics/electronics_Tl208.rml \
 "[REST_VOLUME=electronics;REST_ISOTOPE=Tl208; REST_NEVENTS=100000;REST_FULLCHAIN=off;$GEO;REST_MAXSTEPSIZE=100;REST_EMIN=0;REST_EMAX=50;REST_X=0;REST_Y=0;REST_Z=-1150]"









